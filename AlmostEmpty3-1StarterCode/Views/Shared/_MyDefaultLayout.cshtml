
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    @* ViewBag is a dynamic object. You can create 
        properties with any name you want*@
    @*<title>@ViewBag.Title</title>*@
    <title>@ViewData["Title"]</title>

	@*<link href="~/css/site.css" rel="stylesheet" />*@
	<link href="~/lib/bootstrap-4.4.1-dist/css/bootstrap-Lux.css" rel="stylesheet" />
</head>
<body>
    <div class="text-center">
        <br />

        <h1>My First MVC Website</h1>

		<br />

        <nav>
            <a asp-controller="Home" asp-action="Index">Home Page</a> <br />
            <a asp-controller="Home" asp-action="About">About Page</a> <br />
            <a asp-controller="Home" asp-action="SampleWork">Sample Work Page</a>
        </nav>

		<br />

        <main>
            @RenderBody()
        </main>

		<br />
        <footer>
            <p>&copy;@DateTime.Today.Year David's Website</p>
        </footer>
    </div>
</body>
</html>
